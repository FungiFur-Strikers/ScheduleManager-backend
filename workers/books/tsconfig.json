{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "lib": [
      "ESNext"
    ],
    "types": [
      "@cloudflare/workers-types/2023-07-01"
    ],
    "paths": {
      "@worker/common": ["../common/src"],
      "@worker/common/*": ["../common/src/*"],
      "@project/shared": ["../../packages/shared/src"],
      "@project/shared/*": ["../../packages/shared/src/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx"
  },
}